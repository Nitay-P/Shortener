@{
}
@model UrlInfo
<table class="table table-hover table-dark">
    <thead>
        <tr>
            <th>
                Click Time UTC
            </th>
            <th>
                User IP
            </th>

        </tr>
    </thead>

    <tbody>
       @foreach(var item in Model.ClicksInfo){
                <tr>
                    <td>
                       @item.ClickDateTimeUtc
                    </td>
                    <td>
                      @item.IpAddress
                    </td>
                </tr>            
        }
    </tbody>
</table>